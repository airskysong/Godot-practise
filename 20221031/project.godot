; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="Simple SLG"
run/main_scene="res://Asset/Objects/Level.tscn"
config/icon="res://icon.png"

[autoload]

GameData="*res://Asset/Scripts/GameData.gd"

[debug]

gdscript/warnings/unused_argument=false
gdscript/warnings/return_value_discarded=false

[display]

window/size/width=1600
window/size/height=900
window/size/test_width=800
window/size/test_height=450
window/stretch/mode="2d"
window/stretch/aspect="keep"

[gui]

common/drop_mouse_on_gui_input_disabled=true

[layer_names]

2d_physics/layer_1="Unit"
2d_physics/layer_2="Grid"

[mono]

project/assembly_name="Simple SLG"

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
