; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Reference",
"class": "FileHelper",
"language": "GDScript",
"path": "res://asset/Scripts/FileHelper.gd"
} ]
_global_script_class_icons={
"FileHelper": ""
}

[application]

config/name="first godot"
run/main_scene="res://Application.tscn"
config/use_custom_user_dir=true
config/icon="res://icon.png"

[autoload]

AudioManager="*res://asset/Manager/AudioManager.tscn"
GameData="*res://asset/Scripts/GameData.gd"

[display]

window/size/width=576
window/size/height=1024
window/stretch/mode="2d"
window/stretch/aspect="keep"

[gui]

common/drop_mouse_on_gui_input_disabled=true

[mono]

project/assembly_name="first godot"

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
